{
  "manifest_version": 3,
  "name": "DOM Catcher",
  "version": "1.2",
  "description": "安全的 DOM 元素捕获工具，现在支持同时生成 HTML 和 Markdown 文件！",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:*/",
    "http://127.0.0.1:*/"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
