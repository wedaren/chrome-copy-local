{
  "manifest_version": 3,
  "name": "DOM Catcher",
  "version": "1.1",
  "description": "一键选中 DOM 元素并发送到自定义服务器。",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:*/",
    "http://127.0.0.1:*/",
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
